{
	"name": "frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev-watch": "svelte-check --tsconfig ./tsconfig.json --watch & vite dev",
		"watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@types/pako": "^2.0.4",
		"@vitest/browser": "^3.2.3",
		"jsdom": "^27.0.0",
		"playwright": "^1.53.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0"
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^7.0.0",
		"@tailwindcss/vite": "^4.1.13",
		"pako": "^2.1.0",
		"pixi.js": "^8.13.1",
		"socket.io-client": "^4.8.1",
		"tailwindcss": "^4.1.13"
	}
}
